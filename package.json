{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:https": "concurrently --kill-others-on-fail --names \"next,proxy\" \"next dev --turbopack --hostname 0.0.0.0 --port 3003\" \"local-ssl-proxy --source 3443 --target 3003 --cert .cert/cert.pem --key .cert/key.pem --hostname 0.0.0.0\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "kill:ports": "lsof -ti :3000 -ti :3443 | xargs -r kill -9"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@google-cloud/local-auth": "^3.0.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/material-nextjs": "^7.3.2",
    "@supabase/supabase-js": "^2.58.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "googleapis": "^160.0.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "local-ssl-proxy": "^2.0.5",
    "tw-animate-css": "^1.4.0"
  }
}
